{
  "RabbitMQConnection": {
    "UserName": "guest",
    "Password": "guest",
    "HostName": "localhost",
    "Port": 5672,
    "ExchangeName": "my.exchange",
    
    // RabbitMQ 集群配置（可选）
    // 如果配置了 Address，将自动使用集群模式，忽略上面的 HostName 和 Port
    "Address": [
      "rabbit1.example.com:5672",
      "rabbit2.example.com:5672", 
      "rabbit3.example.com:5672"
    ]
  },
  
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
  "AllowedHosts": "*"
}